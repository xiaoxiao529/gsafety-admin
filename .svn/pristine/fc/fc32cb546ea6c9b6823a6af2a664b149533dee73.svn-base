import _Storage from '../utils/Storage';
export default ({
  app
}) => {
  if (process.client) {

    app.router.beforeEach((to, from, next) => {
      var hasTarget = false;

      function queryList(loopArr, target) {

        for (var i = 0; i < loopArr.length; i++) {
          let item = loopArr[i];
          if (item.resUrl && item.resUrl == target) {
            hasTarget = true;
            return;
          } else {
            if (item.children && item.children.length > 0) {
              queryList(item.children, target);
            }
          }
        }
      }

      const resourceData = _Storage.getObj(
        "jurisdictionList",
        "jurisdictionList"
      ) ? _Storage.getObj(
        "jurisdictionList",
        "jurisdictionList"
      ) : [];


      if (to.name != "login" && to.name != "index" && to.name != "manageIndex" &&
        to.name != "error" &&
        to.name != "informationManager-infoEdit" &&
        to.name != "informationManager-infoAdd" &&
        to.name != "informationManager-infoDetail" &&
        to.name != "downLine-downlinkControlDetail"&&
        to.name != "groupClientManage-assignManage"&&
        to.name != "ownerunitManager-ownerunitEditControl"
      ) {
        queryList(resourceData, to.name);

        if (hasTarget) {
          next();
        } else {
          hasTarget = false;
          next({
            path: '/error'
          })
        }
      } else {
        next()
      }

    })
  }
}
