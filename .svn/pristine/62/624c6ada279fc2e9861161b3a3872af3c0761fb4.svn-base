            <el-radio v-model="isValidArr[0]" label="0" @change="changeOrBlur">否</el-radio>
          </el-form-item>
        </template>
        <el-form-item class="btn-wrapper">
          <el-button
            type="primary"
            :disabled="disbledSubmitFlag"
            @click="LAsubmit"
            v-loading.fullscreen.lock="fullscreenloading"
          >确定</el-button>
          <el-button :disabled="disbledFlag" @click="findMonitorDeviceStateExtByMonitorId()">取消</el-button>
          <el-checkbox v-if="batchFlag" @change="leniaoChecked = !leniaoChecked">同时应用到其他设备</el-checkbox>
        </el-form-item>
      </el-form>
    </template>
    <!--    批量下行-->
    <batch-downlink  ref="batchDownlink" @laBatchDownLink="LAbatchSubmit"></batch-downlink>

    <!--    提醒  弹框-->
    <el-dialog custom-class="del-dialog-wrap"  :visible.sync="czdialogDelVisible" width="340px">
      <div class="del-dialog-main">
        <div class="icon-wrap">
          <i class="el-icon-s-opportunity"></i>
        </div>
        <div class="p-wrap">
          <p class="title-p">还有未获取协议/参数结构的设备。可能会导致数据展示不全。是否要批量获取协议/参数结构？？</p>
        </div>
      </div>
      <span slot="footer" class="dialog-footer">
        <el-button type="primary" @click="getAgreementNumber">获取协议号</el-button>
        <el-button @click="directQuery">直接查询</el-button>
      </span>
    </el-dialog>
  </div>
</template>

<script>
  import batchDownlink from "~/pages/downLine/batchDownlink";
  export default {
    //
    components: {
      batchDownlink
    },
    props: ["fromParent"],
    data() {
      return {
        batchNumber:{
          monitorIds:[],
          controlHosts:[]
        },
        czdialogDelVisible:false,
        batchFlag:false,
        leniaoChecked:false,
        fullscreenloading: false,
        labelPosition: "top",
        isOpenArr: [],
        isValidArr: [],
        isShow: "",
        disbledFlag: false,
        disbledSubmitFlag: true,
        configParameter: {},
        rules: {
          sydlbjz: [
            { required: true, message: "剩余电流报警设置值", trigger: "blur" }
          ],
          wdbjzA: [{ required: true, message: "请输入A相温度", trigger: "blur" }],
          wdbjzB: [{ required: true, message: "请输入B相温度", trigger: "blur" }],
          wdbjzC: [{ required: true, message: "请输入C相温度", trigger: "blur" }],
          wdbjzN: [{ required: true, message: "请输入N相温度", trigger: "blur" }],
          dlbjzA: [{ required: true, message: "请输入A相电流", trigger: "blur" }],
          dlbjzB: [{ required: true, message: "请输入B相电流", trigger: "blur" }],
          dlbjzC: [{ required: true, message: "请输入C相电流", trigger: "blur" }],
          dybjzA: [{ required: true, message: "请输入A相电压", trigger: "blur" }],
          dybjzB: [{ required: true, message: "请输入B相电压", trigger: "blur" }],
          dybjzC: [{ required: true, message: "请输入C相电压", trigger: "blur" }],
          ldjy: [
            {
              required: true,
              message: "请输入漏电阶跃额定值的百分数",
              trigger: "blur"
            }
          ],
          wdjy: [
            {
              required: true,
              message: "请输入温度阶跃额定值的百分数",
              trigger: "blur"
            }
          ],
          dljy: [
            {
              required: true,
              message: "请输入电流阶跃额定值的百分数",
              trigger: "blur"
            }
          ],
          dyjy: [
            {
              required: true,
              message: "请输入电压阶跃额定值的百分数",
              trigger: "blur"
            }
          ],
          zdsy: [
            { required: true, message: "请输入数据上传最大时延", trigger: "blur" }
          ],
          dhbjzA: [
            {
              required: true,
              message: "请输入A相故障电弧报警个数",
              trigger: "blur"
            }
          ],
          dhbjzB: [
            {
              required: true,
              message: "请输入B相故障电弧报警个数",
              trigger: "blur"
            }
          ],
          dhbjzC: [
            {
              required: true,
              message: "请输入C相故障电弧报警个数",
              trigger: "blur"
            }
          ],
          gzdhbjz: [
            {
              required: true,
              message: "请输入故障电弧额定值的百分数",
              trigger: "blur"
            }
          ],
          gzdhbjsjz: [
            { required: true, message: "请输入故障电弧采集时间", trigger: "blur" }
          ],
          sxdyphdbjz: [
            { required: true, message: "请输入电压平衡度设定值", trigger: "blur" }
          ],
          sxdlphdbjz: [
            { required: true, message: "请输入电流平衡度设定值", trigger: "blur" }
          ],
          sxphdbjsj: [
            { required: true, message: "请输入平衡度报警延时", trigger: "blur" }
          ],
          jdq: [{ required: true, message: "请输入继电器", trigger: "blur" }],
          fmq: [{ required: true, message: "请输入蜂鸣器", trigger: "blur" }],
          dylx: [{ required: true, message: "不能为空", trigger: "blur" }],
          dybb: [{ required: true, message: "请输入电压变比", trigger: "blur" }],
          dlbbA: [
            { required: true, message: "请输入第一路电流变比", trigger: "blur" }
          ],
          dlbbB: [
            { required: true, message: "请输入第二路电流变比", trigger: "blur" }
          ],
          dlbbC: [
            { required: true, message: "请输入第三路电流变比", trigger: "blur" }
          ],
          bjys: [{ required: true, message: "请输入报警延时", trigger: "blur" }],
          dglfz: [
            { required: true, message: "请输入大功率负载报警值", trigger: "blur" }
          ],
          dldyxjbfb: [
            {
              required: true,
              message: "请输入短路电压下降百分比",
              trigger: "blur"
            }
          ],
          dldlssbfb: [
            {
              required: true,
              message: "请输入电路电流上升百分比",
              trigger: "blur"
            }
          ],
          ckA: [{ required: true, message: "不能为空", trigger: "blur" }],
          ckB: [{ required: true, message: "不能为空", trigger: "blur" }],
          ckC: [{ required: true, message: "不能为空", trigger: "blur" }],
          dglfz: [{ required: true, message: "不能为空", trigger: "blur" }],
          Adydl: [{ required: true, message: "不能为空", trigger: "blur" }],
          Bdydl: [{ required: true, message: "不能为空", trigger: "blur" }],
          Cdydl: [{ required: true, message: "不能为空", trigger: "blur" }]
        }
      };
    },
    mounted() {
      this.findMonitorDeviceStateExtByMonitorId();
      this.$nextTick(()=>{
        this.batchFlag = (this.fromParent.downType == 1) ? true :false
      })
    },
    methods: {
      /**
       * 批量下行
       * dec: 点击批量下行，判断是否有未获取协议参数的，如果有，就将提示弹框显示。如果没有，批量下发选择框显示
       */
      LAbatchDownlink() {
        const that = this;
        that.fullscreenloading = true;
        this.$axios
          .$POST({
            api_name: "getDeviceInfoByNoVersion",
            params: {
              sourceId: that.fromParent.sourceId,
              deviceTypeId: that.fromParent.deviceTypeId,
              brand: that.fromParent.brand,
              model: that.fromParent.model
            }
          })
          .then(res => {
            if (res.data.code == "success") {
              that.fullscreenloading = false;
              if(res.data.data.length){
                that.czdialogDelVisible = true;
                res.data.data.forEach((item) => {
                   that.batchNumber.monitorIds.push(item.id);
                   that.batchNumber.controlHosts.push(item.controlHost);
                })
              }else {
                that.fullscreenloading = false;
                that.czdialogDelVisible = false;
                that.directQuery();
              }
            } else {
              that.fullscreenloading = false;
              that.$message({
                message: err,
                type: "warning"
              });
            }
          }).catch(err=>{
            that.$message({
              message: err,
              type: "warning"
            });
        });
      },
      /**
       * 批量获取协议参数
       * dec:点击批量获取协议参数提示框里的，获取协议参数，走sendCommandBatchParam接口，接口成功后，批量下发选择框显示。
       */
      getAgreementNumber(){
        const that = this;
        that.fullscreenloading = true;
        this.$axios
          .$POST({
            api_name: "sendCommandBatchParam",
            params: {
              sourceId: that.fromParent.sourceId,
              deviceTypeId: that.fromParent.deviceTypeId,
              brand: that.fromParent.brand,
              model: that.fromParent.model,
              commandType: 2,
              flushThreshold: 1
            },
            time:1000000
          })
          .then(res => {
            if (res.data.code == "success") {
              let tips = `本次操作共${res.data.data.count}条，成功${res.data.data.successNum}条，失败${res.data.data.errorNum}条`;
              that.czdialogDelVisible = false;
              that.$message({
                message: tips,
                type: "success",
                duration:1800
              });
              setTimeout(function () {
                that.fullscreenloading=false;
                that.batchNumber={
                  monitorIds:[],
                  controlHosts:[]
                }
                that.directQuery();
              },2000)
            } else {
              that.fullscreenloading=false;
              that.czdialogDelVisible = true;
              that.$message({
                message: res.data.message || '操作失败',
                type: "warning"
              });
            }
          }).catch(err=>{
          that.fullscreenloading=false;
          that.$message({
            message: err,
            type: "warning"
          });
        });
      },
      /**
       * 直接查询
       */
      directQuery(){
        const that = this;
        that.czdialogDelVisible=false
        that.$refs.batchDownlink.initLaDownLink();
      },
      /**
       * 确认按钮 ，判断批量复选框是否勾选
       */
      LAsubmit(){
        const that = this;
        that.$refs.configParameter.validate(valid => {
          if (valid) {
            this.leniaoChecked ? this.LAbatchDownlink() : this.submitForm();
          }else{
            console.log("error submit!!");
            return false;
          }
        })
      },
      /**
       * 批量下发确定按钮
       * @constructor
       */
      LAbatchSubmit(val1,val2){
        const that = this;
        that.fullscreenloading = true;
        that.configParameter.isOpen = that.isOpenArr.join(""); //isOpenArr拼接成字符串赋值给isOpen
        that.configParameter.isValid = that.isValidArr.join(""); //isValidArr拼接成字符串赋值给isValid
        that.$axios
          .$POST({
            api_name: "sendCommandBatch",
            params: {
              monitorId: val1.join(','),
              controlHost: val2.join(','),
              sourceId: that.fromParent.sourceId,
              commandType: 5,
              params:JSON.stringify(that.configParameter),
              flushThreshold: 1
            }
          })
          .then(res => {
            if (res.data.code == "success") {
              that.fullscreenloading = false;
              let tips = `本次操作共${res.data.data.count}条，成功${res.data.data.successNum}条，失败${res.data.data.errorNum}条`;
              that.$message({
                message: tips,
                type: "success",
                duration:2500
              });
              that.$refs.batchDownlink.batchCancelHandel();
            } else {
              that.fullscreenloading = false;
              this.$message({
                message: "批量下发失败",
                type: "error"
              });
              that.$refs.batchDownlink.LaNumClear();
            }
          }).catch(err=>{
          that.fullscreenloading=false;
          that.$refs.batchDownlink.LaNumClear();
          that.$message({
            message: err,
            type: "warning",
            duration:1800
          });
        });
      },
      changeOrBlur() {
        this.disbledSubmitFlag = false;
      },
      findMonitorDeviceStateExtByMonitorId() {
        //硬件平台-根据监测设备ID获取设备阈值信息---乐鸟
        let that = this;
        this.$axios
          .$POST({
            api_name: "findMonitorDeviceStateExtByMonitorId",
            params: {
              monitorId: that.fromParent.id,
              sourceId: that.fromParent.sourceId
            }
          })
          .then(res => {
            if (res.data.code == "success") {
              that.configParameter = JSON.parse(res.data.data.thresholdValue);
              that.isOpenArr = that.configParameter.isOpen.split("");
              that.isValidArr = that.configParameter.isValid.split("");
            } else {
              console.log("失败");
            }
          });
      },
      resetForm(formName) {
        this.findMonitorDeviceStateExtByMonitorId();
      },
      sendCommand(type) {
        let that = this;
        that.configParameter.isOpen = that.isOpenArr.join(""); //isOpenArr拼接成字符串赋值给isOpen
        that.configParameter.isValid = that.isValidArr.join(""); //isValidArr拼接成字符串赋值给isValid
        that.$refs.configParameter.validate(valid => {
          if (valid) {
            this.$axios
              .$POST({
                api_name: "sendCommand",
                params: {
                  monitorId: that.fromParent.id,
                  controlHost: that.fromParent.controlHost,
                  sourceId: that.fromParent.sourceId,
                  commandType: type,
                  params: type == "5" ? JSON.stringify(that.configParameter) : ""
                }
              })
              .then(res => {
                if (res.data.code == "success") {
                  //如果state不存在 或者state值为空 直接
                  if (!res.data.data.state) {
                    console.log("state为空");
                    this.$message.error("命令发送失败");
                    return;
                  }
                  if (res.data.data.state == "1") {
                    this.$message({
                      message: "命令下发成功",
                      type: "success"
                    });
                    //发送成功后，取消刷新按钮3秒后才能动 防止不入库
                    that.disbledFlag = true;
                    setTimeout(function() {
                      that.disbledFlag = false;
                    }, 3000);
                  } else if (res.data.data.state == "2") {
                    this.$message.error("发送失败");
                  } else if (res.data.data.state == "3") {
                    this.$message({
                      message: "设备不在线或者未接入",
                      type: "warning"
                    });
                  }
                } else {
                  console.log("失败");
                }
              });
            //防止重复点击确定按钮，间隔1秒才能修改
            that.disbledSubmitFlag = true;
            setTimeout(function() {
              that.disbledSubmitFlag = false;
            }, 1000);
          } else {
            console.log("error submit!!");
            return false;
          }
        });
      },
      submitForm() {
        let that = this;
        that.sendCommand(5);
        that.fullscreenloading = true;
        setTimeout(function() {
          that.fullscreenloading = false;
          that.findMonitorDeviceStateExtByMonitorId();
        }, 5000);
      },
      getLatestParameters() {
        //获取最新参数
        let that = this;
        this.$axios
          .$POST({
            api_name: "sendCommand",
            params: {
              monitorId: that.fromParent.id,
              controlHost: that.fromParent.controlHost,
              sourceId: that.fromParent.sourceId,
              commandType: "2",
              flushThreshold: "1"
            }
          })
          .then(res => {
            if (res.data.code == "success") {
              this.$message({
                message: "命令下发成功",
                type: "success"
              });
              setTimeout(function() {
                that.findMonitorDeviceStateExtByMonitorId();
              }, 5000);
            } else {
              console.log("失败");
            }
          });
      },

    }
  };
</script>

