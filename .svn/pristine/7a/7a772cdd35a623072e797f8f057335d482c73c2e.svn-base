.add-btn-style {
  text-align: right;
  padding-right: 25px;
  box-sizing: border-box;
}

.enterpriserUser-wrap {
  padding: 20px;
  display: flex;

  .left-tree {
    flex: 0 0 260px;
    width: 240px;
    transition: all 1s;
    height: 700px;
    overflow: hidden;
    overflow-y: auto;
    padding-right: 10px;

    /deep/.el-tree {
      margin-top: 10px;

      &.el-tree--highlight-current .el-tree-node.is-current>.el-tree-node__content {
        background: transparent;
        color: #66b1ff;
      }


    }
  }

  .right-con {
    flex: 1;
    padding: 0 15px 0;
  }
}

.station-tree-wrap {
  /deep/.el-tree {
    .el-tree__empty-block {
      display: none;
    }
  }

}

.createAdmin-wrap {
  max-height: 550px;

  .el-tree {
    margin-left: 10px;
  }

  /deep/.el-form {
    .el-form-item {
      width: 100%;
      margin-bottom: 20px;

      .el-form-item__label {
        width: 85px;
      }

      .el-form-item__content {
        width: calc(100% - 95px);
      }

      .el-select {
        width: 100%;
      }

      //提示
      &.tip {
        margin: -15px 0 5px 0;

        label {
          opacity: 0;
          line-height: initial;
        }

        .el-form-item__content {
          font-size: 12px;
          color: #b1b1b2;
          line-height: 1.4;
        }
      }

      //底部按钮
      &.btn-item {
        .el-form-item__content {
          text-align: center;
          width: 100%;
        }
      }
    }
  }
}

//用户详情
#userDetail {
  padding: 30px 0px 20px 0;
}

#userDetail .m-content {
  max-height: 134px;
  overflow-y: auto;
}

#userDetail .user-info {
  padding-bottom: 15px;
  overflow-y: hidden;
}

#userDetail .user-info .w-photo {
  float: left;
  display: inline;
  position: relative;
  width: 90px;
}

#userDetail .user-info .photo {
  width: 90px;
  height: 90px;
  border-radius: 50%;
  overflow-y: hidden;
}

#userDetail .user-info .photo img {
  display: block;
  width: 100%;
  height: 100%;
}

#userDetail .user-info .icon-gender {
  position: absolute;
  z-index: 1;
  width: 20px;
  height: 20px;
  right: 8px;
  top: 70px;
  line-height: 20px;
  text-align: center;
  font-size: 14px;
  color: #fff;
  background-color: #4777e7;
  border-radius: 50%;
}

#userDetail .user-info .icon-gender-female {
  background-color: #f5498e;
}

#userDetail .user-info .name {
  height: 14px;
  margin-top: 6px;
  line-height: 14px;
  font-size: 14px;
  color: #232323;
  text-align: center;
}

#userDetail .user-info .base-info {
  margin-left: 120px;
}

#userDetail .user-info .base-info li {
  line-height: 18px;
  padding-bottom: 10px;
  overflow: hidden;
}

#userDetail .user-info .base-info .iconfont {
  float: left;
  display: inline;
  width: 16px;
  font-size: 16px;
  color: #808080;
  margin-right: 8px;
}

#userDetail .user-info .base-info .iconfont.icon-user-line {
  font-size: 18px;
  font-weight: 600;
}

#userDetail .user-info .base-info .iconfont.icon-phone-line {
  font-size: 18px;
}

#userDetail .user-info .base-info .iconfont.icon-mail-line,
#userDetail .user-info .base-info .iconfont.icon-idcard-line {
  font-size: 15px;
}

#userDetail .account-info {
  overflow: hidden;
}

#userDetail .account-info .list {
  float: left;
  display: inline;
  width: 50%;
  line-height: 20px;
  font-size: 14px;
  color: #232323;
}

#userDetail .account-info .list li {
  padding: 5px 0;
}

#userDetail .account-info .label {
  float: left;
  display: inline;
  width: 42px;
  color: #999;
}

#userDetail .account-info .value {
  margin-left: 42px;
  padding-right: 10px;
  line-height: 20px;
}

#userDetail .account-info .i-72 .label {
  width: 72px;
}

#userDetail .account-info .i-72 .value {
  margin-left: 72px;
}

#userDetail .m-content .exit-time {
  height: 34px;
  margin-top: 10px;
  line-height: 34px;
  font-size: 14px;
  color: #232323;
  background-color: #f9f9f9;
}

#userDetail .m-content .exit-time .iconfont {
  margin: 0 8px 0 2px;
  color: #939393;
}

.detail-module-a {
  border: 1px solid #dbdbdb;

  .m-title {
    position: relative;
    height: 40px;
    padding-left: 20px;
    line-height: 40px;
    font-size: 18px;
    color: #35445c;
    background-color: #eef3fe;

    &:before {
      position: absolute;
      z-index: 1;
      content: "";
      width: 3px;
      height: 41px;
      top: -1px;
      left: -1px;
      background-color: #4777e7;
    }
  }

  .m-content {
    padding: 20px;
    max-height: 134px;
    overflow-y: auto;
  }
}

.el-message-box--center .el-message-box__title {
  justify-content: left;

  span {
    margin-top: -13px;
  }
}

//岗位
.station-tree-wrap {
  box-sizing: border-box;
  padding: 10px 0;
  max-height: 120px;
  min-height: 40px;
  border-radius: 4px;
  border: 1px solid #DCDFE6;
  overflow-y: auto;
}
